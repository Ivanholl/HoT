.top-left-corner
.top-right-corner
.bot-left-corner
.bot-right-corner
.border-left
.border-right
.border-top
.border-bot 
.close-btn(ng-click="questsOpen()")
.circle
    img(src="/pictures/icons/quests.png", alt="")
