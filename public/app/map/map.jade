    div
        a.round-button.legend.legendTown
        span.legendText town
        br
        a.round-button.curZone.legend
        span.legendText current location
        br
        a.round-button.selectedZone.legend
        span.legendText selected zone
        br
        a.round-button.grey.legend
        span.legendText under development    
            
    .zone-info
        .top-left-corner
        .top-right-corner
        .bot-left-corner
        .bot-right-corner
        .border-left
        .border-right
        .border-top
        .border-bot
        .text-holder
            |{{zone.name}}
        .zoneDetails
            .text-warning {{zone.info}}            
            .second-row
                table.table-hover.zone-info-table(ng-if="zone.movementOptions.hasBattle")
                    thead
                        tr
                            th.text-primary Minions in this zone
                    tbody
                         tr(ng-repeat="minion in minions")
                            td {{minion.name}}
                img.zone-pic(src="/pictures/backgrounds/{{zone.index}}.jpg")

        .map-btn-holder
            #prev.ornament-green-btn.inline(ng-if="zone.movementOptions.hasPrevZone" ng-class="class" ng-click="prev($event)") Go to {{zone.movementOptions.prevZone}}
            #branch.ornament-green-btn.inline(ng-if="zone.movementOptions.hasBranchZone" ng-class="class" ng-click="branch($event)") Go to {{zone.movementOptions.branchZone}}
            #next.ornament-green-btn.inline(ng-if="zone.movementOptions.hasNextZone" ng-class="class" ng-click="next($event)") Go to {{zone.movementOptions.nextZone}}
            .ornament-red-btn.inline(ng-if="zone.movementOptions.hasBattle" ng-class="class" ng-click="battle($event)") Battle
            .ornament-blue-btn.inline(ng-if="zone.movementOptions.isTown" ng-class="class" ng-click="enter($event)") Enter


    div(ng-include="'partials/chat/chat.jade'")
